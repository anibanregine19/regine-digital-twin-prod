{
  "version": 2,
  "functions": {
    "vercel_mcp_server.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    { "src": "/health", "dest": "/vercel_mcp_server.py" },
    { "src": "/api/test", "dest": "/vercel_mcp_server.py" },
    { "src": "/api/query", "dest": "/vercel_mcp_server.py" },
    { "src": "/", "dest": "/vercel_mcp_server.py" },
    { "src": "/(.*)", "dest": "/vercel_mcp_server.py" }
  ]
}
